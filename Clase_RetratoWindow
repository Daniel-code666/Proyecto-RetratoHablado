    public boolean delete(String name, String password){
        
        boolean find = false;
        
        int i = 0;
        
        while(find == false && i < lista.size())
        {
            if(lista.get(i).getName().compareToIgnoreCase(name) == 0 || lista.get(i).getPassword().compareToIgnoreCase(password) == 0)
            {
                find = true;
            }
            else
            {
                i++;
            }
        }
        if(find)
        {
            JOptionPane.showMessageDialog(null, "Usuario eliminado" + '\n' + lista.get(i).getName() + '\n' + lista.get(i).getPassword());
            lista.remove(i);
            user.setText("");
            pass.setText("");
            return false;       
        }
        else
        {
           JOptionPane.showMessageDialog(null, "No existe.");
           return true;
        }
    }
    
    public boolean search(String name, String password){
        
        boolean find = false;
        
        int i = 0;
        
        while(find == false && i < lista.size())
        {
            if(lista.get(i).getName().compareToIgnoreCase(name) == 0)
            {
                find = true;
            }
            else
            {
                i++;
            }
        }
        if(find)
        {
            JOptionPane.showMessageDialog(null, "Usuario encontrado" + '\n' + lista.get(i).getName() + '\n' + lista.get(i).getPassword());
            user.setText(lista.get(i).getName());
            pass.setText(lista.get(i).getPassword());
            return false;       
        }
        else
        {
           //JOptionPane.showMessageDialog(null, "No existe.");
           return true;
        }
    }

