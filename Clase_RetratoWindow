    String filepathB = "1.wav";
    
    String filepathC = "2.wav";
    
    boolean act = false;
    boolean act1 = false;
    boolean act2 = false;
    
    
    public void actionPerformed (ActionEvent e) {
        	            
        String nameButton = e.getActionCommand();      
        
        ButtonSound musicObjectB = new ButtonSound();
        
        ButtonSound2 musicObjectC = new ButtonSound2();
        
        boolean reg = false;
        if("Register".equalsIgnoreCase(nameButton))
        {
            if(user.getText().isEmpty() || pass.getText().isEmpty())
            {
                JOptionPane.showMessageDialog(null, "Debe igresar todos los datos");
            }
            else
            {
                reg = true;
                act1 = true;
                u.setName(user.getText());
                u.setPassword(pass.getText());
                lista.add(new User(user.getText(), pass.getText()));
                setN.setText(user.getText());
                
                try
                {
                    FileOutputStream fos = new FileOutputStream("C:/Users/danie/Desktop/datosUsuario2.dat");
                    ObjectOutputStream oos = new ObjectOutputStream(fos);
                    oos.writeObject(lista);
                    oos.close();
                    fos.close();
                }
                catch(IOException ioe)
                {
                    ioe.printStackTrace();
                }
               
                btM.setEnabled(true);
                btF.setEnabled(true);
            }
            bt1.setEnabled(false);
            bt2.setEnabled(false);
            bt3.setEnabled(false);
            bt4.setEnabled(false);
            bt5.setEnabled(false);
        }
        
        if("Search".equalsIgnoreCase(nameButton))
        {
            if(user.getText().isEmpty())
            {
                JOptionPane.showMessageDialog(null, "Debe ingresar el campo del nombre");
            }
            else
            {
                search(user.getText(), pass.getText());
                
                try
                {
                    FileInputStream fis = new FileInputStream("C:/Users/danie/Desktop/datosUsuario2.dat");
                    ObjectInputStream ois = new ObjectInputStream(fis);

                    lista = (ArrayList) ois.readObject();

                    ois.close();
                    fis.close();
                } 
                catch (IOException ioe) 
                {
                    ioe.printStackTrace();
                    return;
                } 
                catch (ClassNotFoundException c) 
                {
                    System.out.println("Class not found");
                    c.printStackTrace();
                    return;
                }
                
                Iterator it = lista.iterator();
                while ( it.hasNext() )
                { 
                    Object objeto = it.next(); 
                    User p = (User)objeto;
                    System.out.println(p); 
                } 
            }
            bt1.setEnabled(false);
            bt2.setEnabled(false);
            bt3.setEnabled(false);
            bt4.setEnabled(false);
            bt5.setEnabled(false);
        }
        
        if("Delete".equalsIgnoreCase(nameButton))
        {
            if(user.getText().isEmpty())
            {
                JOptionPane.showMessageDialog(null, "Debe ingresar el campo del nombre");
            }
            else
            {
                delete(user.getText(), pass.getText());
            }
            bt1.setEnabled(false);
            bt2.setEnabled(false);
            bt3.setEnabled(false);
            bt4.setEnabled(false);
            bt5.setEnabled(false);
        }
        
        if("Salir".equalsIgnoreCase(nameButton))
        {
            System.exit(0);
        }
        /*if("Atrás".equalsIgnoreCase(nameButton))
        {
            RetratoLogin winB = null;
            try {
                winB = new RetratoLogin();
            } catch (IOException ex) {
                Logger.getLogger(RetratoWindow.class.getName()).log(Level.SEVERE, null, ex);
            } catch (ClassNotFoundException ex) {
                Logger.getLogger(RetratoWindow.class.getName()).log(Level.SEVERE, null, ex);
            }
            winB.setVisible(true);
            this.setVisible(false);
            
        }*/
        
        if("Comenzar".equalsIgnoreCase(nameButton) && act == false)
        {
            musicObjectC.playSound2(filepathC);
            hair.setVisible(true);
            boca.setVisible(true);
            eyes.setVisible(true);
            cejas.setVisible(true);
            nose.setVisible(true);
            earsM.setVisible(false);
            bt1.setEnabled(true);
            bt2.setEnabled(true);
            bt3.setEnabled(true);
            bt4.setEnabled(true);
            bt5.setEnabled(true);
        }
        else
        {
            musicObjectC.playSound2(filepathC);
            hair.setVisible(true);
            boca.setVisible(true);
            eyes.setVisible(true);
            cejas.setVisible(true);
            nose.setVisible(true);
            earsM.setVisible(true);
            bt1.setEnabled(true);
            bt2.setEnabled(true);
            bt3.setEnabled(true);
            bt4.setEnabled(true);
            bt5.setEnabled(true);
        }
        if("M".equalsIgnoreCase(nameButton))
        {
            musicObjectB.playSound(filepathB);
            
            act = true;
            hair.setBounds(370, 120, 300, 350);
            ImageIcon img1 = new ImageIcon(getClass().getResource("/imagesM/cabello.png"));
            ImageIcon icon = new ImageIcon(img1.getImage().getScaledInstance(hair.getWidth(),hair.getHeight(),Image.SCALE_DEFAULT));
            hair.setIcon(icon);
            btStart.setEnabled(true);
            
            //-Menu-HairM----------------------------------------------------------------------------------------------------------//
            
            l7.setBounds(10, 125, 130, 180);
            ImageIcon img6 = new ImageIcon(getClass().getResource("/imagesM/cabello.png"));
            ImageIcon icon6 = new ImageIcon(img6.getImage().getScaledInstance(l7.getWidth(),l7.getHeight(),Image.SCALE_DEFAULT));
            l7.setIcon(icon6);
            l7.setVisible(false);
            bt6.setVisible(false);
            bt6.setEnabled(false);
            
            
            l8.setBounds(10, 315, 130, 180);
            ImageIcon img7 = new ImageIcon(getClass().getResource("/imagesM/cabello2.png"));
            ImageIcon icon7 = new ImageIcon(img7.getImage().getScaledInstance(l8.getWidth(),l8.getHeight(),Image.SCALE_DEFAULT));
            l8.setIcon(icon7);
            l8.setVisible(false);
            bt7.setVisible(false);
            bt7.setEnabled(false);
            
            l9.setBounds(10, 505, 130, 180);
            ImageIcon img8 = new ImageIcon(getClass().getResource("/imagesM/cabello5.png"));
            ImageIcon icon8 = new ImageIcon(img8.getImage().getScaledInstance(l9.getWidth(),l9.getHeight(),Image.SCALE_DEFAULT));
            l9.setIcon(icon8);
            l9.setVisible(false);
            bt8.setVisible(false);
            bt8.setEnabled(false);
            
            //---------------------------------------------------------------------------------------------------------------------//
            
            boca.setBounds(900, 400, 165, 60);
            ImageIcon img2 = new ImageIcon(getClass().getResource("/imagesM/boca4.png"));
            ImageIcon icon2 = new ImageIcon(img2.getImage().getScaledInstance(boca.getWidth(),boca.getHeight(),Image.SCALE_DEFAULT));
            boca.setIcon(icon2);
            
            //-Menu-BocaM----------------------------------------------------------------------------------------------------------//
            
            l13.setBounds(-10, 175, 165, 60);
            ImageIcon img18 = new ImageIcon(getClass().getResource("/imagesM/boca4.png"));
            ImageIcon icon18 = new ImageIcon(img18.getImage().getScaledInstance(l13.getWidth(),l13.getHeight(),Image.SCALE_DEFAULT));
            l13.setIcon(icon18);
            l13.setVisible(false);
            bt9.setVisible(false);
            bt9.setEnabled(false);

            l14.setBounds(10, 365, 155, 85);
            ImageIcon img19 = new ImageIcon(getClass().getResource("/imagesM/boca2.png"));
            ImageIcon icon19 = new ImageIcon(img19.getImage().getScaledInstance(l14.getWidth(),l14.getHeight(),Image.SCALE_DEFAULT));
