    public void setName(){
        
        Container container = getContentPane();
        
        btSave = new JButton("Register");
        btSave.setBounds(60, 55, 85, 20);
        btSave.setFont(new Font("consolas",Font.PLAIN,10));
        btSave.setForeground(Color.black);
        btSave.setVisible(true);
        container.add(btSave);
        btSave.addActionListener(this);
        
        btDel = new JButton("Delete");
        btDel.setBounds(60, 75, 85, 20);
        btDel.setFont(new Font("consolas",Font.PLAIN,10));
        btDel.setForeground(Color.black);
        btDel.setVisible(true);
